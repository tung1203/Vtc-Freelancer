					<div class="inside-post-request">
						@if(@ViewBag.UserName!= null)
						{
						<b>Hi , @ViewBag.UserName</b><br /><br />
						}
						<p>Get offer from sellers for your project</p>
						<br /><br />
						<button type="button" onclick="window.location.href = '/manager_request'">Post a
							request</button>
					</div>
				</div>
				<div class="slide-section">
					<div class="slideshow-container">

						<div class="mySlides fade">
							<div class="numbertext">1 / 3</div>
							<img class="img-slide" src="~/images/slide1.png" style="width:100%">
							<div class="text">Caption Text</div>
						</div>

						<div class="mySlides fade">
							<div class="numbertext">2 / 3</div>
							<img class="img-slide" src="~/images/slide2.png" style="width:100%">
							<div class="text">Caption Two</div>
						</div>

						<div class="mySlides fade">
							<div class="numbertext">3 / 3</div>
							<img class="img-slide" src="~/images/slide3.png" style="width:100%">
							<div class="text">Caption Three</div>
						</div>

						<a class="prev" onclick="plusSlides(-1)">&#10094;</a>
						<a class="next" onclick="plusSlides(1)">&#10095;</a>

					</div>
					<br>

					<div style="text-align:center">
						<span class="dot" onclick="currentSlide(1)"></span>
						<span class="dot" onclick="currentSlide(2)"></span>
						<span class="dot" onclick="currentSlide(3)"></span>
					</div>
				</div>



			</section>
		</div>
