@{
ViewData["Title"] = "Seller Dashboard";
Layout = "~/Views/Shared/_SellerLayout.cshtml";
}

<html lang="en">

<body>
    <!-- <p id="dmm" style="padding: 10px; background: red;">dmmmmmmmmmmmm</p> -->
    <div class="content-seller">
        <div class="body-content container">
            <div class="content left-content">
                <div class="container-profile">
                    <div class="profile-wrapper">
                        <label class="lable-profile">
                            @if(ViewBag.userAvatar == null)
                            {
                            <a href="/@ViewBag.UserName"><img class="img-profile" src="~/Images/avatardefault.png" alt="avatar"></a>
                            }
                            else
                            {
                            <a href="/@ViewBag.UserName"><img class="img-profile" src="~/@ViewBag.userAvatar" alt="avatar"></a>
                            }
                        </label>
                        <br>
                        <br>
                        <p class="username">@ViewBag.UserName</p>
                        <br>

                    </div>
                    <ul class="ul-profile bderbtm">
                        <li class="li-profil">
                            <a href="#">
                                <h3>Response Rate</h3>
                            </a>
                        </li>
                        <li class="li-profil">
                            <a href="#">
                                <h3>Delivered on Time</h3>
                            </a>
                        </li>
                        <li class="li-profil">
                            <a href="#">
                                <h3>Order Completion</h3>
                            </a>
                        </li>
                    </ul>
                    <ul class="ul-profile">
                        <li class="li-profil">
                            <a href="#">
                                <h3>Earned in November</h3>
                            </a>
                        </li>
                        <li class="li-profil">
                            <a href="#">
                                <h3>Response Time</h3>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="right-content">
                <div class="update-bussiness"><b>
                        <h2>Upgrade Your Business</h2>
                        <br>
                    </b>
                    <div class="content slide-upgrade">
                        <div class="slide-section slide-upgrade">
                            <div class="slide-container container">
                                <div class="left-content left-reponsive">
                                    <div class="smail-content">
                                        <div class="logo">

                                            <div class="icon-slide-uprage icon1"></div>

                                            <div class="icon-slide-uprage icon2"></div>
                                            <div class="icon-slide-uprage icon3"></div>
                                        </div>

                                    </div>
                                </div>
                                <div class="right-content">
                                    <div class="slideshow-container slide-space">

                                        <div class="mySlides fade">
                                            <p class="title-slide">Communicate With Your Customers</p>
                                            <br>
                                            <div>Good communication is a key factor for creating successful
                                                collaborations. Reply quickly to all your customers’
                                                inquiries, provide information and proactively update on your progress
                                                throughout the entire order process.</div>
                                        </div>

                                        <div class="mySlides fade">

                                            <p class="title-slide">Take Courses On VTC Freelancer Today</p>
                                            <br>
                                            <div>Put your professional career in full force with courses on Learn
                                                from VTC Freelancer. Gain new skills, earn a badge on your
                                                profile, and keep those sales rolling in.</div>
                                        </div>

                                        <div class="mySlides fade">

                                            <p class="title-slide">Business Software for Freelancers</p>
                                            <br>
                                            <div>AND CO helps you with everything from invoicing, payment to time
                                                and task tracking. Use code “VTC15” for 15% off your
                                                first year</div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <br>

                            <div style="text-align:center">
                                <span class="dot" onclick="currentSlide(1)"></span>
                                <span class="dot" onclick="currentSlide(2)"></span>
                                <span class="dot" onclick="currentSlide(3)"></span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>


</html>